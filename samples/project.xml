<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="ZYGameEngineHaxe" package="com.sample.zygameenginehaxe" version="1.0.0" company="Company Name" />
	<app main="Main" path="Export" file="ZYGameEngineHaxe" />
	
	<template path="templates"/>

	<define name="spine38"/>

	<define name="spine3.8" if="spine38"/>

	<assets path="Assets" rename="assets"/>
	
	<haxelib name="openfl"/>
	<haxelib name="openfl-spine"/>
	<haxelib name="spine-hx" version="3.6.0" unless="spine38"/>
	<haxelib name="spine-hx" if="spine38"/>
	<source path="Source" />

	<!-- <window allow-high-dpi="true" /> -->
	<haxedef name="gl_stats"/>

	<!-- <haxelib name="qqplaycore"/> -->
	<set name="VIEW_MODE" value="1"/>


</project>